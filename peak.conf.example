  status_text: '>Listening on 146.52Mhz http://JeffreyFreeman.me'
  id_text: 'WI2ARD/30M1 GATE/2M1 WI2ARD-1/2M1 WIDEN-n IGATE'
  id_path: ['WIDE1-1', 'WIDE2-2']
  old_paradigm: ['ECHO', 'PA']
