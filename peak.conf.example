TNC KENWOOD:
  com_port: /dev/ttyUSB1
  baud: 9600
  parity: none
  stop_bits: 1
  byte_size: 8
  port_count: 1
  kiss_init: MODE_INIT_KENWOOD_D710

TNC RPR:
  com_port: /dev/ttyUSB0
  baud: 38400
  parity: none
  stop_bits: 1
  byte_Size: 8
  port_count: 1
